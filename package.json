{
  "name": "formr",
  "version": "1.1.0",
  "description": "Formr with riot 4",
  "private": true,
  "scripts": {
    "dev": "(start npm run watch && start npm run server) || (npm run watch & npm run server)",
    "build": "rollup -c",
    "watch": "rollup -c -w 'src/*'",
    "server": "browser-sync start --files 'dist/*, index.html' --server"
  },
  "dependencies": {
    "marked": "^0.6.3",
    "riot": "^4.3.7"
  },
  "devDependencies": {
    "@riotjs/compiler": "^4.3.7",
    "browser-sync": "^2.26.7",
    "rollup": "^1.18.0",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-riot": "^4.0.0"
  },
  "license": "MIT"
}
